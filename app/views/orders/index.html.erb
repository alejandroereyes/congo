<table class="table table-hover">
  <thead>
    <tr>
      <th>Order #</th>
      <th>Customer</th>
      <th>Total</th>
    </tr>
  </thead>
  <tbody>
  <% @orders.each do |order| %>
    <tr>
      <td><%= order.id %></td>
      <td><%= order.user.inspect %></td>
    </tr>
  <% end %>
  </tbody>
</table>
<%= will_paginate @orders %>